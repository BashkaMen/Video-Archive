<Application
    x:Class="VideoArchive.App.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:VideoArchive.App"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Colors.xaml" />
                <ResourceDictionary Source="Styles\Fonts.xaml" />
                <ResourceDictionary Source="Styles\Icons.xaml" />

                <ResourceDictionary Source="Styles\Defaults.xaml" />
                <ResourceDictionary Source="Styles\Texts.xaml" />
                <ResourceDictionary Source="Styles\Buttons.xaml" />
                <ResourceDictionary Source="Styles\ContentControls.xaml" />
                <ResourceDictionary Source="Styles\UserControl.xaml" />

            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>











    </Application.Resources>
</Application>
